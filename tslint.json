{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-line-length": [true, 200],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "align": false,
    "object-curly-spacing": false
  }
}
