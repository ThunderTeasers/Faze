{
  "compilerOptions": {
    "outDir": "./dist/",
    "strictNullChecks": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es2016",
      "es2017",
      "esnext"
    ],
    "module": "es6",
    "target": "es6",
    "downlevelIteration": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "./src/**/*"
  ]
}
